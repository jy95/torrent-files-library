{
  "name": "torrent-files-library",
  "version": "1.5.0",
  "description": "Scan directories to build a library of media files (movie or tv show) that follows torrent naming conventions",
  "main": "index.js",
  "files": [
    "index.js",
    "lib/"
  ],
  "scripts": {
    "semantic-release": "semantic-release -e ./config/release.config.js",
    "compile": "babel -d lib/ src/ --source-maps both --no-comments",
    "prepare": "npm run compile",
    "test": "nyc ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage-html": "nyc report --reporter=html",
    "esdoc": "node_modules/.bin/esdoc",
    "lint": "./node_modules/.bin/eslint -c config/.eslintrc.js ./src ./test --fix"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=5"
  },
  "keywords": [
    "torrent",
    "library",
    "parser",
    "torrent file",
    "torrent files",
    "parse torrent",
    "parse torrent file",
    "parse torrent files",
    "parse torrent name",
    "torrent library"
  ],
  "author": "jy95",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jy95/torrent-files-library.git"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^1.0.0",
    "@semantic-release/git": "^3.0.0",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "conventional-changelog-eslint": "^1.0.0",
    "coveralls": "^3.0.0",
    "esdoc": "^1.0.4",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.8.0",
    "nyc": "^11.4.1",
    "semantic-release": "^13.0.2",
    "sinon": "^4.2.2"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "filehound": "^1.16.2",
    "lodash": "^4.17.4",
    "parse-torrent-title": "^1.1.0",
    "video-extensions": "^1.1.0"
  }
}
