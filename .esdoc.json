{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {"name": "esdoc-standard-plugin",
      "option": {
        "test": {
          "source": "./test/",
          "interfaces": ["test"],
          "excludes": ["_constants.js"]
        }
      }
    },
    {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}}
  ]
}