{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": [ "./src" ]
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/minami",
    "readme": "examples.md",
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  }
}